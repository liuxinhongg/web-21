<view class="banner">
  <swiper indicator-dots="true" autoplay="autoplay" interval="1000" duration="500">
    <block >
      <swiper-item>
        <image src="../../image/banner/background.png"></image>
      </swiper-item>
      <swiper-item>
        <image src="../../image/banner/img1.png"></image>
      </swiper-item>
      <swiper-item>
        <image src="../../image/banner/img12.png"></image>
      </swiper-item>
      <swiper-item>
        <image src="../../image/banner/img13.png"></image>
      </swiper-item>
    </block>
  </swiper>
</view>
